{
  "name": "palomita",
  "version": "1.0.0",
  "description": "Palomita est un blog personnel",
  "main": "index.js",
  "scripts": {
    "wp:web:dev": "webpack --env development --module web",
    "wp:backoffice:dev": "webpack --env development --module backoffice",
    "wp:dev": "webpack --env development --module app",
    "wp:web:prod": "webpack --env production --module web",
    "wp:backoffice:prod": "webpack --env production --module backoffice",
    "wp:prod": "webpack --env production --module app",
    "server:web:prod": "node backend/app.js --env production --module web",
    "server:backoffice:prod": "node backend/app.js --env production --module backoffice",
    "server:prod": "node backend/app.js --env production --module app",
    "app:web:dev": "nodemon backend/app.js --env development --module web --watch frontend/views/web --watch frontend/config --watch backend",
    "app:backoffice:dev": "nodemon backend/app.js --env development --module backoffice --watch frontend/views/backoffice --watch frontend/config --watch backend",
    "app:dev": "nodemon backend/app.js --env development --module app --watch frontend/views --watch frontend/config --watch backend",
    "app:web:prod": "npm run wp:web:prod && npm run server:web:prod",
    "app:backoffice:prod": "npm run wp:backoffice:prod && npm run server:backoffice:prod",
    "app:prod": "npm run wp:prod && npm run server:prod"
  },
  "repository": {
    "type": "git",
    "url": "github.com/vioule/palomita"
  },
  "keywords": [
    "palomita",
    "paloma",
    "blog",
    "nodejs",
    "reactjs",
    "node",
    "react"
  ],
  "author": "Vincent Cano",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "array-move": "^2.2.1",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "mini-css-extract-plugin": "^0.7.0",
    "multer": "^1.4.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "pug-loader": "^2.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-sortable-hoc": "^1.11.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bson": "^4.0.3",
    "connect-flash": "^0.1.1",
    "csurf": "^1.10.0",
    "del": "^5.1.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "minimist": "^1.2.0",
    "mongoose": "^5.6.11",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.4.0"
  }
}
